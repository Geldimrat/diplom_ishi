.wp-embed-share-dialog { position: absolute; inset: 0px; background-color: rgba(0, 0, 0, 0.9); color: rgb(255, 255, 255); opacity: 1; transition: opacity 0.25s ease-in-out 0s; }

.wp-embed-share-dialog.hidden { opacity: 0; visibility: hidden; }

.wp-embed-share-dialog-close, .wp-embed-share-dialog-open { margin: -8px 0px 0px; padding: 0px; background: 0px 0px; border: none; cursor: pointer; outline: 0px; }

.wp-embed-share-dialog-close .dashicons, .wp-embed-share-dialog-open .dashicons { padding: 4px; }

.wp-embed-share-dialog-open .dashicons { top: 8px; }

.wp-embed-share-dialog-close:focus .dashicons, .wp-embed-share-dialog-open:focus .dashicons { box-shadow: rgb(79, 148, 212) 0px 0px 0px 1px, rgba(79, 148, 212, 0.8) 0px 0px 2px 1px; border-radius: 100%; }

.wp-embed-share-dialog-close { position: absolute; top: 20px; right: 20px; font-size: 22px; }

.wp-embed-share-dialog-close:hover { text-decoration: none; }

.wp-embed-share-dialog-close .dashicons { height: 24px; width: 24px; background-size: 24px; }

.wp-embed-share-dialog-content { height: 100%; transform-style: preserve-3d; overflow: hidden; }

.wp-embed-share-dialog-text { margin-top: 25px; padding: 20px; }

.wp-embed-share-tabs { margin: 0px 0px 20px; padding: 0px; list-style: none; }

.wp-embed-share-tab-button { display: inline-block; }

.wp-embed-share-tab-button button { margin: 0px; padding: 0px; border: none; background: 0px 0px; font-size: 16px; line-height: 1.3; color: rgb(167, 170, 173); cursor: pointer; transition: color 0.1s ease-in 0s; }

.wp-embed-share-tab-button [aria-selected="true"] { color: rgb(255, 255, 255); }

.wp-embed-share-tab-button button:hover { color: rgb(255, 255, 255); }

.wp-embed-share-tab-button + .wp-embed-share-tab-button { margin: 0px 0px 0px 10px; padding: 0px 0px 0px 11px; border-left: 1px solid rgb(167, 170, 173); }

.wp-embed-share-tab[aria-hidden="true"] { display: none; }

p.wp-embed-share-description { margin: 0px; font-size: 14px; line-height: 1; font-style: italic; color: rgb(167, 170, 173); }

.wp-embed-share-input { box-sizing: border-box; width: 100%; border: none; height: 28px; margin: 0px 0px 10px; padding: 0px 5px; font-size: 14px; font-weight: 400; font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell, "Helvetica Neue", sans-serif; line-height: 1.5; resize: none; cursor: text; }

textarea.wp-embed-share-input { height: 72px; }

